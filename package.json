{
  "name": "boilerplate-web",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "vite --port=3000",
    "build": "vite build",
    "build:source-map": "vite build --sourcemap=true",
    "preview": "vite preview --port=3001",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write \"**/*.+(ts|tsx|json|less|html|md)\"",
    "prepare": "husky install",
    "analyze": "source-map-explorer 'build/assets/*.js'"
  },
  "dependencies": {
    "@ant-design/icons": "^4.7.0",
    "@tanstack/react-query": "^4.14.1",
    "@tanstack/react-query-devtools": "^4.14.1",
    "ahooks": "^3.7.2",
    "antd": "^4.24.0",
    "axios": "^1.1.3",
    "date-fns": "^2.25.0",
    "i18next": "^22.0.4",
    "i18next-http-backend": "^2.0.1",
    "js-cookie": "^3.0.1",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "query-string": "^7.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.0.0",
    "react-router-dom": "^6.4.3",
    "web-vitals": "^1.1.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^27.5.1",
    "@types/js-cookie": "^3.0.2",
    "@types/lodash": "^4.14.187",
    "@types/node": "^16.11.7",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "less": "^4.1.3",
    "lint-staged": "^11.2.6",
    "prettier": "^2.7.1",
    "source-map-explorer": "^2.5.2",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.4",
    "vite": "^3.2.2"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,json,css,less,md}": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
